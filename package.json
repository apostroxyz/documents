{
  "name": "apostro-demo",
  "version": "0.0.1",
  "scripts": {
    "postinstall": "yarn yarn-deduplicate yarn.lock",
    "dev": "yarn clean && better-npm-run dev",
    "build": "yarn clean && better-npm-run build",
    "lint": "eslint . --ext .js,.jsx,.ts,.tsx && tsc --noEmit --incremental false --pretty",
    "lint:autofix": "eslint . --ext .js,.jsx,.ts,.tsx --fix",
    "clean": "rm -rf .next",
    "tsc:watch": "tsc --watch --noEmit --incremental false --pretty"
  },
  "betterScripts": {
    "node": {
      "command": "node ./node_modules/.bin/cross-env",
      "env": {
        "TS_NODE_PROJECT": "webpack/tsconfig.json",
        "TS_NODE_FILES": true,
        "NODE_OPTIONS": "--max_old_space_size=12288"
      }
    },
    "build": {
      "command": "better-npm-run node next build"
    },
    "dev": {
      "command": "next dev -p 8083"
    }
  },
  "license": "MIT",
  "dependencies": {
    "@babel/core": "^7.19.3",
    "@next/bundle-analyzer": "^12.0.10",
    "better-npm-run": "^0.1.1",
    "build-route-tree": "^1.0.2",
    "core-decorators": "^0.20.0",
    "file-loader": "^6.2.0",
    "next": "^12.0.10",
    "react": "^17.0.2",
    "react-dom": "^17.0.2",
    "reflect-metadata": "^0.1.13",
    "ts-toolbelt": "^9.6.0"
  },
  "devDependencies": {
    "@babel/plugin-proposal-decorators": "^7.17.0",
    "@types/react-dom": "^17.0.11",
    "@typescript-eslint/eslint-plugin": "^5.10.2",
    "@typescript-eslint/parser": "^5.10.2",
    "@vercel/node": "^2.5.14",
    "babel-plugin-transform-typescript-metadata": "^0.3.2",
    "cross-env": "^7.0.3",
    "enzyme": "^3.11.0",
    "enzyme-to-json": "^3.6.2",
    "eslint": "^8.8.0",
    "eslint-config-airbnb": "^19.0.4",
    "eslint-config-airbnb-typescript": "^16.1.0",
    "eslint-config-prettier": "^8.3.0",
    "eslint-plugin-import": "^2.25.4",
    "eslint-plugin-json": "^3.1.0",
    "eslint-plugin-jsx-a11y": "^6.5.1",
    "eslint-plugin-prettier": "^4.0.0",
    "eslint-plugin-react": "^7.28.0",
    "eslint-plugin-react-hooks": "^4.3.0",
    "prettier": "^2.5.1",
    "tsconfig-paths": "^3.12.0",
    "typescript": "^4.5.5",
    "yarn-deduplicate": "^3.1.0"
  },
  "engines": {
    "node": ">=14.0.0"
  }
}
